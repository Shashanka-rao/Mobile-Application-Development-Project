<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5F7D95"
    tools:context=".MainClasses.ContactUs">

    <TextView
        android:id="@+id/headingCU"
        android:layout_width="289dp"
        android:layout_height="127dp"

        android:layout_marginStart="100dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="100dp"
        android:background="@drawable/edittextradius"
        android:backgroundTint="#2BC48A"
        android:paddingTop="40dp"
        android:text="CONTACT US"
        android:textAlignment="center"
        android:textColor="#23001E"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:layout_width="372dp"
        android:layout_height="537dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/edittextradius"
        app:cardCornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headingCU"
        app:layout_constraintVertical_bias="0.95" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="50dp"
                android:textSize="35dp"
                android:backgroundTint="#EFEFEF"
                android:background="@drawable/edittextradius"
                android:textColor="#292829"
                android:text="Emergency Service"
                android:paddingLeft="30dp"

                />

            <TextView
                android:id="@+id/emergencyDial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="7898849483"

                android:paddingLeft="40dp"
                android:textColor="#464F51"
                android:textSize="20dp"/>

            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:background="@drawable/edittextradius"
                android:backgroundTint="#EFEFEF"
                android:paddingLeft="30dp"
                android:text="Ambulance"
                android:textColor="#292829"
                android:textSize="35dp" />

            <TextView
                android:id="@+id/ambulanceDial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="108"

                android:paddingLeft="40dp"
                android:textColor="#464F51"
                android:textSize="20dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textSize="35dp"
                android:text="Police"
                android:backgroundTint="#EFEFEF"
                android:background="@drawable/edittextradius"
                android:paddingLeft="30dp"
                android:textColor="#292829"
                />

            <TextView
                android:id="@+id/policeDial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="100"

                android:paddingLeft="40dp"
                android:textColor="#464F51"
                android:textSize="20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textSize="35dp"
                android:text="Fire Safety"
                android:backgroundTint="#EFEFEF"
                android:background="@drawable/edittextradius"
                android:textColor="#292829"
                android:paddingLeft="30dp"
                />

            <TextView
                android:id="@+id/fireSDial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="101"

                android:paddingLeft="40dp"
                android:textColor="#464F51"
                android:textSize="20dp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textSize="35dp"
                android:text="Tow"
                android:backgroundTint="#EFEFEF"
                android:background="@drawable/edittextradius"
                android:paddingLeft="30dp"
                android:textColor="#292829"
                />

            <TextView
                android:id="@+id/towDial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="8947743322"

                android:textColor="#464F51"
                android:paddingLeft="40dp"
                android:textSize="20dp"/>

            <TextView

                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:textSize="35dp"
                android:paddingLeft="30dp"
                android:backgroundTint="#EFEFEF"
                android:background="@drawable/edittextradius"
                android:text="Developers"
                android:textColor="#292829"
                />

            <TextView
                android:id="@+id/d1Dial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:paddingLeft="40dp"
                android:textColor="#464F51"

                android:text="8105476973" />
            <TextView
                android:id="@+id/d2Dial"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:paddingLeft="40dp"
                android:textColor="#464F51"

                android:text="9740234550" />


        </LinearLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>


